{
	"name": "url-navigation",
	"displayName": "常用网站RSS",
	"description": "Surf in the vscode",
	"publisher": "VitoZhang",
	"icon": "assets/imgs/RSS.png",
	"keywords": [
		"RSS",
		"摸鱼",
		"导航",
		"网址",
		"fish",
		"上班"
	],
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:treeView-data",
		"onCommand:itemClick"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "navigation",
					"title": "Navigation",
					"icon": "$(globe)"
				}
			]
		},
		"views": {
			"navigation": [
				{
					"id": "treeView-data",
					"name": "阅读"
				},
				{
					"id": "treeView-data2",
					"name": "刷题"
				},
				{
					"id": "treeView-data3",
					"name": "面试"
				},
				{
					"id": "treeView-data4",
					"name": "博客"
				},
				{
					"id": "treeView-data5",
					"name": "刷题"
				},
				{
					"id": "treeView-data6",
					"name": "摄影"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p -w . --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/content-type": "^1.1.5",
		"@types/follow-redirects": "^1.13.1",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.3",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"content-type": "^1.0.4",
		"follow-redirects": "^1.14.5"
	}
}
